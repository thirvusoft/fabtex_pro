 "css": ".master{\n    width:100%;\n    border:1px solid black;\n    font-family:arial;\n    font-size:13px;\n}\n\n.master table{\n    font-family:arial;\n    font-size:12px;\n    color:black;\n    width:100%;\n}\n\n.right1 tr{\n    border-bottom:1px solid black;\n    \n    \n}\n\n.leftline{\n    border-left:1px solid black;\n    \n}\n\n.left1{\n    float:left;\n    width:50%;\n}\n\n.right1{\n    float:right;\n    width:50%;\n}\ntable .hsn{\n    width:100;\n}\n.hsn tr{\n    border-top:1px solid black;\n}\n.print-format td{\n    padding: 2px!important; border-spacing: 0!important;  margin:0!important; height:0!important;padding-left: 4px!important;padding-right: 3px!important;\n}\n.print-format{\n    margin-top:5px;\n}.master{\n    width:100%;\n    border:1px solid black;\n    font-family:arial;\n    font-size:13px;\n}\n\n.master table{\n    font-family:arial;\n    font-size:12px;\n    color:black;\n    width:100%;\n}\n\n.right1 tr{\n    border-bottom:1px solid black;\n}\n\n.leftline{\n    border-left:1px solid black;\n}\n\n.left1{\n    float:left;\n    width:50%;\n}\n\n.right1{\n    float:right;\n    width:50%;\n}\ntable .hsn{\n    width:100;\n}\n.hsn tr{\n    border-top:1px solid black;\n}\n.print-format td{\n    padding: 2px!important; border-spacing: 0!important;  margin:0!important; height:0!important;padding-left: 4px!important;padding-right: 3px!important;\n}\n.print-format{\n    margin-top:5px;\n}",
